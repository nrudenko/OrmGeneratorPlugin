public enum @SCHEMA_NAME@ {

@COLUMNS@

    private String columnName;
    private util.DBType type;

    S(String columnName, util.DBType type) {
        this.columnName = columnName;
        this.type = type;
    }

    public String getName() {
        return columnName;
    }

    public util.DBType getType() {
        return type;
    }

    public int position() {
        return this.ordinal();
    }

    public String toString() {
        return this.getName();
    }
}

