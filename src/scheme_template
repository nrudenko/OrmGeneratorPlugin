package @PACKAGE@;

import @DB_TYPE_CLASS@;

public enum @SCHEMA_NAME@ {

@COLUMNS@

    private String columnName;
    private DBType type;

    @SCHEMA_NAME@(String columnName, DBType type) {
        this.columnName = columnName;
        this.type = type;
    }

    public String getName() {
        return columnName;
    }

    public DBType getType() {
        return type;
    }

    public int position() {
        return this.ordinal();
    }

    public String toString() {
        return this.getName();
    }
}